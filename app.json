{
  "expo": {
    "name": "SKI'UTC",
    "slug": "skiutc",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/app.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "fr.utc.assos.skiutc",
      "permissions": ["CAMERA"],
      "config": {
        "googleMaps": { "apiKey": "" }
      },
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "fr.utc.assos.skiutc",
      "buildNumber": "5",
      "infoPlist": {
        "NSCameraUsageDescription": "L'application utilise la cam√©ra pour poster changer son avatar",
        "NSPhotoLibraryUsageDescription": "L'application utilise la gallerie photo pour poster des news en ajoutant une photo ou pour changer son avatar"
      },
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "notification": {
      "icon": "./assets/images/app.png"
    }
  }
}
